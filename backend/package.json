        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.3"
    },
    "scripts": {
        "start-server": "node builds/src/server.js",
        "start-worker": "node builds/src/events/subscriber/keyword-event.js",
        "server:dev": "ts-node-dev --poll --transpile-only --respawn src/server.ts --watch src/server.ts",
        "worker:dev": "ts-node-dev --poll --transpile-only --respawn src/events/subscriber/keyword-event.ts --watch src/events/subscriber/keyword-event.ts",
        "build": "npm i && tsc && npm run copy-docs",
        "build:dev": "tsc",
        "copy-docs": "cp -r docs builds/",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --config .prettierrc.js --write './**/*.{js,ts,json,yml,yaml}' --ignore-path .prettierignore",
        "format:check": "prettier --config .prettierrc.js --check './**/*.{js,ts,json,yml,yaml}' --ignore-path .prettierignore",
        "test": "jest ./tests/**/**.ts",
        "generate-error-map": "ts-node src/cmd/generate-error-map generate"
    },
