        "express-openapi-validator": "^5.1.3",
        "helmet": "^7.1.0",
        "http-graceful-shutdown": "^3.1.13",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.3",
        "socket.io": "^4.7.5",
        "source-map-support": "^0.5.21",
        "swagger-ui-express": "^5.0.0",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@apidevtools/json-schema-ref-parser": "^11.1.0",
        "@jest/reporters": "^29.7.0",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
